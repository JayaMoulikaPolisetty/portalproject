package Mainprograms;

public class Prime100 {
	
	public static void main(String[] args) {

		int limit =0, j =2;
		
		for (int i = 2; ;i++)
		{
			int count = 0;
			for(;j<i;j++)
			{
				if(i%j==0)
				{
					count++;
				}
			}
			if(count>0)
			{
				System.out.print(" "+j);
				limit++;
			}
			
		}
		
	}

	
}
